{
  "name": "gelato-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "bulma": "0.7.5",
    "classnames": "2.2.6",
    "dotenv": "8.0.0",
    "faker": "^4.1.0",
    "gelato-errors": "file:../gelato-errors",
    "gelato-validation": "file:../gelato-validation",
    "qrious": "^4.0.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-google-maps": "^9.4.5",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "react-stripe-checkout": "2.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --runInBand",
    "eject": "react-scripts eject",
    "test:debug": "react-scripts debug test --runInBand --no-cache",
    "test:coverage": "react-scripts test --coverage --runInBand --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gelato-data": "file:../gelato-data",
    "jsonwebtoken": "8.5.1",
    "node-sass": "4.12.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/api/*.js",
      "src/logic/*.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}
